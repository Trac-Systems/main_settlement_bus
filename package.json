        "test:bare:cov": "brittle-bare -c -t 120000 test/all.test.js"
    },
    "dependencies": {
        "@tracsystems/blake3": "^0.0.6",
        "autobase": "7.16.1",
        "b4a": "1.6.7",
        "bare-fs": "^4.1.6",
        "bare-readline": "1.0.7",
        "bare-tty": "5.0.2",
        "bech32": "^2.0.0",
        "brittle": "^3.17.1",
        "compact-encoding": "^2.16.0",
        "corestore": "^7.4.2",
        "fastest-validator": "1.19.0",
        "https": "npm:bare-node-https@^1.0.0",
        "hyperbee": "^2.24.2",
        "hypercore": "^11.6.3",
        "hypercore-crypto": "^3.4.0",
        "hyperdht": "^6.20.5",
