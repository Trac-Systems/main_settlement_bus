{
    "name": "trac-msb",
    "main": "msb.mjs",
    "version": "0.1.77",
    "pear": {
        "name": "trac-msb",
        "type": "terminal"
    },
    "type": "module",
    "scripts": {
        "dev": "pear run -d .",
        "dev-rpc": "pear run -d . ${npm_config_store} --rpc",
        "protobuf": "node scripts/generate-protobufs.js",
